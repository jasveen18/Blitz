		 <script src="https://js.stripe.com/v3/"></script>
		 
	</head>
	<body>

			<nav class="nav-main">
				<a href='/'><h3 class="logo" style='font-weight: 800px;'>Blitz</h3></a>
				<ul>
				   <li> <a href="/login">Login</a></li>
				   <li> <a href="/register">Sign Up</a></li>
				   <li> <a href="/about">About</a></li>
				   <li> <a href="/contact">Contact</a></li>
				   <li> <a href="/logout">Logout</a></li>
				</ul>
			</nav>
		
		<div class="toggle">
			<button id="hit">
				<svg  viewBox="0 -2 100 80" width="40" height="30"style="overflow: visible;"> 
					<rect id="change0"width="75" height="10" fill="#e6ffff"></rect>
					<rect id="change" y="30" width="75" height="10"fill="#e6ffff"></rect>
					<rect id="change1" y="60" width="75" height="10"fill="#e6ffff"></rect>
		        </svg>  
			</button>
		</div>

		<script type="text/javascript" src="/scripts/nav.js"></script>
		

	<%if(error && error.length>0){%>
		<div class="container">
			<div class="alert alert-danger">
			<%=error%>
			</div>
		</div>
	<%}%>
		<% if(success && success.length > 0){ %>
		<div class="container">
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
		</div>
        <% } %>
